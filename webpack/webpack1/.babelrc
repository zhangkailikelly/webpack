{
  "presets": [
    "react",
    "es2015",
    "stage-0"
  ],
  "plugins": [
    "transform-object-assign",
    "add-module-exports",
    "transform-object-rest-spread",
    [
      "import",
      {
        "libraryName": "antd",
        "libraryDirectory": "lib",
        "style": "css"
      },
      {
        "libraryName": "antd-mobile",
        "libraryDirectory": "component",
        "style":"css"
      }
    ]
  ],
  "env": {
    "development": {
      "presets": [
        "react-hmre"
      ]
    }
  }
}